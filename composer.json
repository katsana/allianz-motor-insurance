{
    "name": "katsana/allianz-motor-insurance",
    "description": "PHP Agnostic library for working with Allianz Motor Insurance",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Allianz\\MotorInsurance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Allianz\\MotorInsurance\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "laravie/codex": "^4.0 || ^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "php-http/curl-client": "^1.7 || ^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
